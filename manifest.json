{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "2.0.0",
  "key": "",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "permissions": ["storage", "tabs", "identity"],
  "oauth2": {
    "client_id": "169990336049-2qbinsm16kduu9f5k1uq514103gujf1b.apps.googleusercontent.com",
    "scopes": ["openid", "email", "profile"]
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "快速收藏当前页面"
    }
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon16.png", "icon48.png", "icon128.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 